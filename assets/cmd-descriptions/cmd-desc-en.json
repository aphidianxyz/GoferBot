{
    "commands": [
        {
            "commandName": "about",
            "commandSyntax": "/about",
            "commandDesc": "describes GoferBot, and links to the project page",
            "params": null
        },
        {
            "commandName": "caption",
            "commandSyntax": "/caption [URL] [\"top\"] [\"bot\"]",
            "commandDesc": "creates a caption meme",
            "params": [
                {
                    "paramName": "URL",
                    "paramDesc": "a URL pointing to an image"
                },
                {
                    "paramName": "\"top\"",
                    "paramDesc": "the top caption, encased by quotes"
                },
                {
                    "paramName": "\"bot\"",
                    "paramDesc": "the bot caption, encased by quotes"
                }
            ]
        },
        {   
            "commandName": "caption",
            "commandSyntax": "/caption (with an attached image) [\"top\"] [\"bot\"]",
            "commandDesc": "creates a caption meme",
            "params": [
                {
                    "paramName": "\"top\"",
                    "paramDesc": "the top caption, encased in quotes"
                },
                {
                    "paramName": "\"bot\"",
                    "paramDesc": "the bot caption, encased in quotes"
                }
            ]
        },
        {   
            "commandName": "caption",
            "commandSyntax": "/caption (while replying to a sticker/image) [\"top\"] [\"bot\"]",
            "commandDesc": "creates a caption meme",
            "params": [
                {
                    "paramName": "\"top\"",
                    "paramDesc": "the top caption, encased in quotes"
                },
                {
                    "paramName": "\"bot\"",
                    "paramDesc": "the bot caption, encased in quotes"
                }
            ]
        },
        {   
            "commandName": "everyone",
            "commandSyntax": "/everyone",
            "commandDesc": "pings everyone (that has messaged at least once since the bot was added)",
            "params": null
        },
        {   
            "commandName": "hello",
            "commandSyntax": "/hello",
            "commandDesc": "has Goferbot greet you",
            "params": null
        },
        { 
            "commandName": "help",
            "commandSyntax": "/help [command?]",
            "commandDesc": "lists all commands, what they do and their syntax",
            "params": [
                {
                    "paramName": "command",
                    "paramDesc": "(optional) a specific command to describe"
                }
            ]
        },
        {
            "commandName": "creategroup",
            "commandSyntax": "/creategroup [name]",
            "commandDesc": "creates a group, which can be pinged using /mention",
            "params": [
                {
                    "paramName": "groupName",
                    "paramDesc": "a name to assign to a new group"
                }
            ]
        },
        {
            "commandName": "join",
            "commandSyntax": "/join [group]",
            "commandDesc": "join a specified group",
            "params": [
                {
                    "paramName": "group",
                    "paramDesc": "the name of the group to join"
                }
            ]
        },
        {
            "commandName": "leave",
            "commandSyntax": "/leave [group]",
            "commandDesc": "leave a specified group",
            "params": [
                {
                    "paramName": "group",
                    "paramDesc": "the name of the group to join"
                }
            ]
        },
        {
            "commandName": "groups",
            "commandSyntax": "/groups",
            "commandDesc": "lists all the groups in the current chat",
            "params": null
        },
        {
            "commandName": "mention",
            "commandSyntax": "/mention [group]",
            "commandDesc": "pings all users in a group",
            "params": [
                {
                    "paramName": "group",
                    "paramDesc": "the name of the group to ping"
                }
            ]
        },
        {   
            "commandName": "pin",
            "commandSyntax": "/pin (while replying to a message) -notify",
            "commandDesc": "pins a replied message",
            "params": [
                {
                    "paramName": "-notify",
                    "paramDesc": "an optional flag to notify all users in the chat of the pin"
                }
            ] 
        },
        {   
            "commandName": "ping",
            "commandSyntax": "/ping",
            "commandDesc": "has GoferBot respond with \"pong\"",
            "params": null
        }
    ]
}
